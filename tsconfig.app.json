/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
     "paths": {
    "@pages/*": ["app/pages/*"],
    "@components/*": ["app/components/*"],
    "@services/*": ["app/services/*"],
    "@models/*": ["app/models/*"]
  }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
